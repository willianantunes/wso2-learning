<?xml version="1.0" encoding="UTF-8"?>
<task class="org.apache.synapse.startup.tasks.MessageInjector"
  group="synapse.simple.quartz" name="My_ESB_Task" xmlns="http://ws.apache.org/ns/synapse">
  <trigger count="10" interval="10000"/>
  <property name="injectTo" value="sequence" xmlns:task="http://www.wso2.org/products/wso2commons/tasks"/>
  <property name="message" xmlns:task="http://www.wso2.org/products/wso2commons/tasks">
    <name xmlns="">Next	message	will	be	in	10	secs!</name>
  </property>
  <property name="sequenceName" value="main" xmlns:task="http://www.wso2.org/products/wso2commons/tasks"/>
</task>
